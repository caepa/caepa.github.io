
{
  "@context":"http://schema.org",
  "@type":"WebSite",
  "name":"{{ site.title }}",
  "alternateName":"{{ site.legal_name}}",
  "url":"{{ site.baseurl | prepend: site.url }}",
  "publisher":[
    {
      "@type":"Organization",
      "logo":{
        "@type":"ImageObject",
        "url":"{{ site.logo | prepend: site.baseurl | prepend: site.url }}",
        "width":254,
        "height":60
      },
      "name":"{{ site.title }}",
      "legalName":"{{ site.legal_name }}",
      "description":"{{ site.mission }}",
      "foundingDate":"1935",
      "areaServed":"Colorado",
      "url":"{{ page.url | prepend: site.baseurl | prepend: site.url }}",
      "sameAs":[
        "{{ site.baseurl | prepend: site.url }}",{% for media in site.social %}
        "{{ media.url}}"{% unless forloop.last %},{% endunless %}{% endfor %}
      ],
      "subOrganization":[  
        "COABE",
        "MPAEA"
      ]
    }
  ]
}
